CREATE SCHEMA `emailqueue` ;

CREATE TABLE `emailqueue`.`mail` (
  `ID` INT NOT NULL AUTO_INCREMENT COMMENT '',
  `FROM` VARCHAR(100) NOT NULL COMMENT '',
  `TO` VARCHAR(100) NOT NULL COMMENT '',
  `SUBJECT` VARCHAR(500) NULL COMMENT '',
  `BODY` VARCHAR(10000) NULL COMMENT '',
  `SENT_STATUS` INT NULL DEFAULT 0 COMMENT '',
  PRIMARY KEY (`ID`)  COMMENT '');
